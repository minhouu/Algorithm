package sol2;

import java.util.Scanner;

public class Main {
	static int MAX = 100000;
	static int[][] arr = new int[2][MAX];
	static int[][] memo = new int[MAX][3];
	static int N;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for (int t = 0; t < T; t++) {
			N = sc.nextInt();
			// takes input
			for (int i = 0; i < 2; i++) {
				for (int j = 0; j < N; j++) {
					arr[i][j] = sc.nextInt();
				}
			}
			// initialize memo with -1
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < 3; j++) {
					memo[i][j] = -1;
				}
			}
			
		}
	sc.close();
	}
	
}
