package sol2;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// 바텀업으로 간다
		int max = 1000001;
		int[] dp = new int[max];

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		for (int i = 0; i < n; i++) {
			dp[i + 1] = dp[i] + 1;
			if (dp[i * 2] < n) {
				dp[i * 2] = Math.min(dp[i] + 1, dp[2 * i - 1] + 1);
			}
		}
	}
}
