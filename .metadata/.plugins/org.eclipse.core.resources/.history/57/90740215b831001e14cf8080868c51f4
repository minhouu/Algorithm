package sol1;

public class hanoi {
	public static void main(String[] args) {
		
	}
	public void f(int n, String from, String to, String via) {
		if(n==1) {
			System.out.println("Move " + n + " from " + from  + " to " + to);
		}
		f(n-1, from, via, to);
		System.out.println("Move " + n + " from " + from  + " to " + to);
		f(n-1, via, to, from);
	}
}
